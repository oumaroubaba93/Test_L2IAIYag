import org.junit.jupiter.api.Test;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class MultiplicationTableTest {

    @Test
    public void testMultiplicationTable() {
        MultiplicationTable multiplicationTable = new MultiplicationTable();

        // Tester la table de multiplication du nombre 5
        int number = 5;
        int[] expectedResults = {5, 10, 15, 20, 25, 30, 35, 40, 45, 50};

        int[] actualResults = multiplicationTable.getMultiplicationTable(number);

        // Vérifier que les résultats réels correspondent aux résultats attendus
        assertEquals(expectedResults, actualResults);
    }
}
